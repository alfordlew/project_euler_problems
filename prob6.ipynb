   "source": [
    "def sum_squares(x):\n",
    "    sum_square = 0\n",
    "    square_sum = 0\n",
    "    for i in range(1,x+1):\n",
    "        sum_square += i**2\n",
    "        square_sum += i\n",
    "    square_sum = square_sum**2\n",
    "    return square_sum - sum_square"
   ]
